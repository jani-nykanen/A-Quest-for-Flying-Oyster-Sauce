# Game assets

@path assets/bitmaps/
@type bitmap
{
    player player.png
    tiles1 tiles_1.png
    sky1 sky_1.png
    clouds1 clouds_1.png
    font font.png
    boulder boulder.png
    key key.png
    star star.png
    lock lock.png
    icons icons.png
    compl complete.png
    enemy enemies.png
    bigStar big_star.png
}

@path assets/maps/
@type tilemap
{
    testMap test.tmx
}

@path assets/audio/
@type music
{
    theme theme.ogg
    clear clear.ogg
}

@path assets/audio/
@type sample
{
    jump jump.wav
    die die.wav
    thwomp thwomp.wav
    transf transform.wav
    getKey key.wav
    openLock lock.wav
    push push.wav
    accept accept.wav
    select select.wav
    pause pause.wav
    restart restart.wav
}